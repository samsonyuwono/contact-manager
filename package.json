{
  "name": "contact-manager",
  "description": "Getting started with contact manager app",
  "license": "Apache-2.0",
  "devDependencies": {
    "@hubspot/cli": "latest",
    "@hubspot/prettier-plugin-hubl": "latest",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "prettier": "^2.7.1",
    "yarpm": "^1.2.0"
  },
  "scripts": {
    "start": "cd contact-manager-project/contact-manager-app && npm start",
    "postinstall": "cd contact-manager-project/contact-manager-app && yarpm install",
    "lint:js": "eslint . --ext .js,.jsx",
    "prettier": "prettier . --check",
    "watch:hubl": "hs watch contact-manager-theme contact-manager-theme",
    "upload:hubl": "hs upload contact-manager-theme contact-manager-theme",
    "deploy": "hs project upload contact-manager-project"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
